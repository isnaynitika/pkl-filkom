{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Our Work.Portofolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "gambarporto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "judulporto": {
      "type": "string"
    },
    "nama_aplikasi": {
      "type": "string"
    },
    "perusahaan": {
      "type": "string"
    },
    "penjelasan": {
      "type": "text"
    },
    "solusi": {
      "type": "text"
    },
    "fitur_portos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::fitur-porto.fitur-porto",
      "mappedBy": "portfolio"
    },
    "galeriportofolios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::galeriportofolio.galeriportofolio",
      "mappedBy": "portfolio"
    },
    "hasil_portos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::hasil-porto.hasil-porto",
      "mappedBy": "portfolio"
    },
    "slug": {
      "type": "uid",
      "targetField": "judulporto"
    }
  }
}
